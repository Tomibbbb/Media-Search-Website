# Openverse API Credentials
# Register for credentials at https://api.openverse.org/v1/#tag/auth

# Copy this file to .env and fill in your actual credentials
OPENVERSE_CLIENT_ID=your_openverse_client_id
OPENVERSE_CLIENT_SECRET=your_openverse_client_secret

# MongoDB Connection URI
MONGODB_URI=mongodb://localhost:27017/myapp

# JWT Secret Key
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=1d

# Google OAuth Credentials
# Register at https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_CALLBACK_URL=http://localhost:3000/api/auth/google/callback

# Frontend URL for redirects after OAuth
FRONTEND_URL=http://localhost:3000